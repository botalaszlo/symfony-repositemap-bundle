<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for entityUrlGroup in urls %}
        {% for entityUrl in entityUrlGroup %}
            <url>
                <loc>{{ 'http://www.' ~ app.request.host ~ entityUrl.url }}</loc>
                <lastmod>{{ entityUrl.lastmod }}</lastmod>
                <changefreq>{{ entityUrl.changefreq }}</changefreq>
                <priority>{{ entityUrl.priority }}</priority>
            </url>
        {% endfor %}
    {% endfor %}
</urlset>