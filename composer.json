{
  "name": "symfony-repositemap-bundle",
  "description": "RepoSiteMapbundle is a symfony 2 bundle which provides creating sitemap.xml from static urls and from entity repository.",
  "keywords": ["symfony2", "bundle", "sitemap", "xml"],
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Bóta László",
      "email": "bota.laszlo.dev@outlook.com"
    }
  ],
  "support": {
    "issues": "https://github.com/botalaszlo/symfony-repositemap-bundle/issues"
  },
  "require": {
    "php": ">=5.5.9",
    "symfony/symfony": "3.0.*",
    "sensio/framework-extra-bundle": "^3.0.2",
    "doctrine/orm": "^2.5",
    "doctrine/doctrine-bundle": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "5.1.*@stable"
  },
  "autoload": {
    "psr-4": {
      "RepoSitemapBundle\\": ""
    },
    "classmap": [
      "Tests/app/AppKernel.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "RepoSitemapBundle\\": "",
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "extra": {
  	"branch-alias": {
  		"dev-master": "1.1-dev"
  }
}
}
